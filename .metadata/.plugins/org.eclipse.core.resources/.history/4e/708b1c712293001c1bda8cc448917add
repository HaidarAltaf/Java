package vehicles;

public class Sports extends Garage {
	private String engine;	

	public String getEngine() {
		return engine;
	}
	public void setEngine(String engine) {
		this.engine = engine;
	}

	public Sports(String name, int price, boolean running, String engine) {
		super(name, price, running);
		this.engine = engine;
	}
	
	@Override
	public String toString() {
		return "Sports car engine= " + engine;
	}
}
