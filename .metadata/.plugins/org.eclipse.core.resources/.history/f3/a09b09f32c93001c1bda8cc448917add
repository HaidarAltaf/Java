package vehicles;

import java.util.ArrayList;

public class Garage {
	private ArrayList<Vehicles> 
	vehicles = new ArrayList<Vehicles>();
	
	public void addVehicle(Vehicles vehicles) {
		this.vehicles.add(vehicles);
			System.out.println(vehicles);
	}
	
	public void deleteVehicle(Vehicles vehicles) {
		this.vehicles.remove(vehicles);
			System.out.println(vehicles);
	}
	
	public void sizeVehicle(Vehicles vehicles) {
		this.vehicles.size();
	}
	
	public void clearVehicle(Vehicles vehicles) {
		this.vehicles.clear();
			System.out.println(vehicles);
	}
	
	
}
