package vehicles;

public class classics extends Garage{
	private int year;

	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
	public classics(String name, int price, boolean running, int year) {
		super(name, price, running);
		this.year = year;
	}
	@Override
	public String toString() {
		return "classic: year= " + year;
	}
	
	
}
