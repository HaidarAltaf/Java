package garage.runner;

import java.util.ArrayList;
import java.util.List;

public class runnerGarage {
	public static void main(String[] args) {
		List<String> Garage = new ArrayList<>();
		for(int i = 0; i < Garage.size(); i++)
			Garage.add(" ");
			Garage.add("Koenigsegg Regera");
			Garage.add("Veilside Acura NSX");
			Garage.add("Mercedes 300SL Gullwing");
			Garage.add("Rolls-Royce Wraith");
		for(String j : Garage) {
			System.out.println(j);
		}
			Garage.remove(1);
	}
}
