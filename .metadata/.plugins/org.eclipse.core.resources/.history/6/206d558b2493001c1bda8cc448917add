package vehicles;

public class luxury extends Garage{
	private boolean elite;

	public luxury(String name, double price, boolean running, int id, boolean elite) {
		super(name, price, running, id);
		this.elite = elite;
	}

	public boolean getElite() {
		return this.elite;
	}

	public void setElite(boolean elite) {
		this.elite = elite;
	}
	
	@Override
	public String toString() {
		return "luxury? " + elite + ", running? " + isRunning() + ", Price " + getPrice() + ", Name "
				+ getName() + ", is Elite? " + getElite();
	}

	
	}
	

