package stringmanipulation;

public class Exercise {
	private String name;
	private String fluffiness;
	private String breed; 
	private boolean isGoodDog;
	private int age;
	
	public Exercise() {}
	
	public Exercise(String name, String fluffiness, String breed, boolean isGoodDog, int age) {
		this.name = name;
		this.fluffiness = fluffiness;
		this.breed = breed;
		this.isGoodDog = isGoodDog;
		this.age = age;
	}
	
}
