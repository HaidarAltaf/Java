package vehicles;

public class luxury extends Garage{
	private boolean elite;

	public luxury(String make, int price, boolean runner, int id, boolean elite) {
		super(make, price, runner, id);
		this.elite = elite;
	}

	public boolean getElite() {
		return this.elite;
	}

	public void setElite(boolean elite) {
		this.elite = elite;
	}
	
	@Override
	public String toString() {
		return "luxury? " + elite + ", running? " + isRunning() + ", Price " + getPrice() + ", Name "
				+ getName() + ", toString()=";
	}

	
	}
	

